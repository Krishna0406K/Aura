# Aura App Environment Configuration
# Copy this file to .env and update with your values

# Backend API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:8080
EXPO_PUBLIC_WEBSOCKET_URL=ws://localhost:8080

# Development Settings
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Feature Flags
EXPO_PUBLIC_ENABLE_VOICE_RECOGNITION=true
EXPO_PUBLIC_ENABLE_AI_FEATURES=true
EXPO_PUBLIC_ENABLE_TRANSLATION=true

# Google Cloud Configuration (for AI features)
EXPO_PUBLIC_GOOGLE_PROJECT_ID=your-google-project-id
EXPO_PUBLIC_GOOGLE_LOCATION=us-central1
EXPO_PUBLIC_GOOGLE_AGENT_ID=your-dialogflow-agent-id